{
	"schema_version": 1,
	"quilt_loader": {
		"group": "${group}",
		"id": "auramancy",
		"version": "${version}",
		"metadata": {
			"name": "Auramancy",
			"description": "A magic mod for Minecraft focused on channeling and distilling the aspects of Aura to create powerful items and devices.",
			"contributors": {
				"Penguin_Spy": "Owner"
			},
			"contact": {
				"homepage": "https://penguinspy.dev/projects/auramancy/",
				"issues": "https://github.com/Penguin-Spy/auramancy/issues",
				"sources": "https://github.com/Penguin-Spy/auramancy"
			},
			"license": "MPL-2.0-no-copyleft-exception",
			"icon": "assets/auramancy/icon.png"
		},
		"intermediate_mappings": "net.fabricmc:intermediary",
		"entrypoints": {
			"init": "dev.penguinspy.auramancy.Auramancy"
		},
		"depends": [
			{
				"id": "quilt_loader",
				"versions": ">=0.19.1"
			},
			{
				"id": "quilted_fabric_api",
				"versions": ">=7.0.2"
			},
			{
				"id": "minecraft",
				"versions": ">=1.20"
			}
		]
	},
	"mixin": "auramancy.mixins.json"
}
